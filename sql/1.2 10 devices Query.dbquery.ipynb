   "source": [
    "-- Count messages per device (top 10)\n",
    "SELECT\n",
    "  device_id,\n",
    "  COUNT(*) AS messages\n",
    "FROM workspace.bronze.device_messages_raw\n",
    "GROUP BY device_id\n",
    "ORDER BY messages DESC\n",
    "LIMIT 10;\n",
